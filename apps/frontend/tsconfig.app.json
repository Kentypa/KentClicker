{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@icons/*": [
        "assets/icons/*"
      ],
      "@fonts/*": [
        "assets/fonts/*"
      ],
      "@images/*": [
        "assets/images/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@enums/*": [
        "enums/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@interceptors/*": [
        "interceptors/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@stores/*": [
        "stores/*"
      ],
      "@custom-types/*": [
        "types/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@ui/*": [
        "components/UI/*"
      ],
      "@selectors/*": [
        "stores/selectors/*"
      ],
      "@screens/*": [
        "screens/*"
      ]
    },
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true
  },
  "include": [
    "src",
    "aliases.json",
    "scripts"
  ]
}